<!--<h1>Detail for public</h1> -->
<div class="wrapper">
    <div>
        <h2><%= name %></h2>
        <h3><%= category %></h3>
    </div>
    
    <div><img src="<%= image %>" ></div>
    <ul>
        <% ingredients.forEach(element => { %>
            <li><%= element.ingredient %> </li>
        <% }) %>
    </ul>
    <button><a href="/user/details-private/<%= id %>">See more infomation</a></button>
</div>