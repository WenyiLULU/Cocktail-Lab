<h1>Cocktail Lab</h1>
<div>
    <div>
        <h2>Alcoholic</h2>
        <div class="cocktailList">
            <% cocktailAlcToShow.forEach(({name, image}) => { %>
                <div>
                    <div><img src="<%= image %> " alt="Cocktail Image" height="200px"></div>
                    <h3><%= name %></h3>                    
                </div>
            <% }) %>
            
        </div>
    </div>
    <div>
        <h2>No Alcoholic</h2>
        <div class="cocktailList">
            <% cocktailNAlcToShow.forEach(({name, image}) => { %>
                <div>
                    <div><img src="<%= image %> " alt="Cocktail Image" height="200px"></div>
                    <h3><%= name %></h3>                    
                </div>
            <% }) %>
        </div>
    </div>
</div>
